class SomeMath {
    public static void main(String[] args) {
        int a = 1, b = 2, c = 3, max, min;

        if (a > b && a > c) {
            max = a;
            if (b > c) {
                min = c;
            } else {
                min = b;
            }
        } else if (b > c) {
            max = b;
            if (a > c) {
                min = c;
            } else {
                min = a;
            }
        } else {
            max = c;
            if (a > b) {
                min = b;
            } else {
                min = a;
            }
        }

        System.out.println(min);
        System.out.println(max);
    }
}
